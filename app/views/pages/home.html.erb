<h1>Midpoint Home Page</h1>
<div class="row">
  <div class="col-sm-8 my-3">
    <%= form_with url: meetups_path, method: :post, class: "d-flex" do %>
    <%= text_field_tag :query,
      params[:query],
      class: "form-control p-5",
      placeholder: "Please enter an airport code"
    %>
    <%= submit_tag "Search", name: "", class:"btn btn-primary" %>
    <% end %>
  </div>
</div>
