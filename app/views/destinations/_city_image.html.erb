<% unless destination.airport_to.country_code == "us" || destination.airport_to.country_code == "ca" %>
  <%= image_tag "https://images.skypicker.com/?image=https%3A%2F%2Fimages.kiwi.com%2Fphotos%2F1280x720%2F#{destination.fly_to_city.downcase.parameterize}_#{destination.airport_to.country_code.downcase}.jpg", alt: "logo of #{destination.fly_to_city}", class: "summary-img"%>
<% else %>
  <% if @destination.airport_to.city_photo.attached? %>
    <%= cl_image_tag @destination.airport_to.city_photo.key, alt: "photo of #{@destination.airport_to.city_name}", class: "summary-img" %>
  <% else %>
    <%= image_tag "stock/oslo-stock.jpg", alt: "photo of #{@destination.airport_to.city_name}", class: "summary-img" %>
  <% end %>
<% end %>
